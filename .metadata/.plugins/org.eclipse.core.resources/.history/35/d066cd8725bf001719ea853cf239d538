package com.userfront.controller;

import org.springframework.*;





@Controller
public class HomeController {
	
	@RequestMapping("/")
	public String home() {
		return "redirect:/index";
	}
	
	@RequestMapping("/index")
	public String index() {
		return "index"; //index.html
	}
	
	@RequestMapping(value = "/signup", method = RequestMethod.
	
	
}
